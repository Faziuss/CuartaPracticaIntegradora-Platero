<h1>Carrito de Compras</h1>

<div id="cartsContainer">
  {{#each cart.products}}
    <div>
      <li>ID del Producto: {{this.product._id}}</li>
      <li>Titulo: {{this.product.title}}</li>
      <li>Cantidad: {{this.quantity}}</li>
    </div>
  {{/each}}

  <button id="buyCartBtn" data-cartId="{{cart._id}}">Comprar</button>

</div>

<script src="/js/carts.js"></script>
<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>